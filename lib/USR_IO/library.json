{
    "name": "USR_IO",
    "version": "1.0.0",
    "description": "AWS IOT test library for ESP32",
    "keywords": "communication, AWS, IoT",
    "repository": {
      "type": "git",
      "url": "https://exploreembedded.com/wiki/AWS_IOT_with_Arduino_ESP32"
    },
    "authors": [
      {
        "name": "ExploreEmbedded.com",
        "email": "support@exploreembedded.com",
        "maintainer": true
      }
    ],
    "license": "MIT",
    "frameworks": "arduino",
    "platforms": "espressif32",
    "build": {
      "srcDir": "src",              
      "srcFilter": "+<*>"           
    },
    "export": {
      "include": "src/USR_IO.h",    
      "flags": [
        "-Lsrc/esp32",              
        "-lUSR_IO"                 
      ]
    }
  }